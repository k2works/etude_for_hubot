{
  "name": "etude-for-hubot",
  "version": "0.0.0",
  "private": true,
  "author": "k2works <kakimomokuri@gmail.com>",
  "description": "A simple helpful robot for your Company",
  "scripts": {
    "build": "babel src --out-dir scripts",
    "test": "eslint src && flow && mocha",
    "precommit": "yarn test",
    "prepush": "yarn test",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "prod:build": "rimraf scripts && babel src -d scripts",
    "prod:start": "pm2 start process.json",
    "prod:stop": "pm2 stop process.json"
  },
  "dependencies": {
    "aws2js": "^0.8.3",
    "hubot": "^2.19.0",
    "hubot-diagnostics": "0.0.2",
    "hubot-google-images": "^0.2.7",
    "hubot-google-translate": "^0.2.1",
    "hubot-help": "^0.2.2",
    "hubot-heroku-keepalive": "^1.0.3",
    "hubot-maps": "0.0.3",
    "hubot-pugme": "^0.1.1",
    "hubot-redis-brain": "0.0.4",
    "hubot-rules": "^0.1.2",
    "hubot-scripts": "^2.17.2",
    "hubot-shipit": "^0.2.1",
    "hubot-slack": "^4.4.0",
    "hubot-zabbix-notifier": "^0.0.1",
    "moment": "^2.20.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "blanket": "^1.2.3",
    "chai": "^4.1.2",
    "co": "^4.6.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.66.0",
    "hubot-matteruser": "^4.4.0",
    "hubot-test-helper": "^1.8.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "jscoverage": "^0.6.0",
    "mocha": "^5.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodeunit": "^0.11.2",
    "pm2": "^2.10.1",
    "rimraf": "^2.6.2"
  }
}
